          "./node_modules/ng-file-upload/dist/ng-file-upload.min.js",
          "./node_modules/ng-file-upload/dist/ng-file-upload-shim.min.js"
        ],
        "css": [
          "./node_modules/bootstrap/dist/css/bootstrap.min.css",
          "./node_modules/angular-toastr/dist/angular-toastr.min.css",
          "./node_modules/animate.css/animate.min.css",
          "./node_modules/font-awesome/css/font-awesome.min.css",
          "./client/css/*.css",
          "./node_modules/deepsense-loading-spinner/dist/deepsense-loading-spinner.css",
          "./node_modules/deepsense-catalogue-panel/dist/catalogue-panel.css",
          "./node_modules/deepsense-context-menu/dist/context-menu.css",
          "./node_modules/deepsense-attributes-panel/dist/deepsense-attributes-panel.css",
          "./node_modules/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.css",
          "./node_modules/ng-switchery/dist/switchery.min.css",
          "./node_modules/deepsense-navigation-panel/dist/deepsense-navigation-panel.css"
        ]
      }
    },
    "tests": {
      "client": "./client/app/**/*.spec.js",
      "e2e": "./e2e-tests/**/*.spec.js"
    }
  }
}
