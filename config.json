{
  "env": {
    "frontend": {
      "host": "http://localhost",
      "port": 8000
    },
    "proxy": {
      "host": "http://localhost",
      "port": 8001
    }
  },
  "files": {
    "build": {
      "path": "./build/",
      "js": "js/",
      "css": "css/",
      "fonts": "fonts/",
      "images": "img/",
      "bundle": {
        "app": "app.js",
        "js": "libs.js",
        "css": "libs.css",
        "partials": "partials.js"
      }
    },
    "client": {
      "path": "./client/",
      "app": "app/app.js",
      "js": "app/**/*.js",
      "less": "css/**/*.less",
      "lessSources": "**/*.less",
      "html": "**/*.html",
      "fonts": "fonts/*",
      "images": "img/*",
      "favicon": "favicon.ico",
      "assets": "./**/assets/**/*"
    },
    "server": {
      "path": "./server/",
      "app": "app.js",
      "js": "**/*.js"
    },
    "libs": {
      "dev": {
        "js": [
          "./node_modules/angular/angular.js",
          "./node_modules/angular-ui-router/release/angular-ui-router.js",
          "./node_modules/angular-bootstrap/ui-bootstrap.js",
          "./node_modules/angular-bootstrap/ui-bootstrap-tpls.js",
          "./node_modules/deepsense-context-menu/dist/context-menu.js",
          "./bower_components/jsPlumb/dist/js/dom.jsPlumb-1.7.4.js",
          "./bower_components/mousetrap/mousetrap.js",
          "./bower_components/lodash/lodash.js",
          "./bower_components/jquery/dist/jquery.min.js",
          "./bower_components/highcharts-release/highcharts.js",
          "./bower_components/highcharts-release/highcharts-more.js",
          "./node_modules/ng-debounce/dist/ng-debounce.js",
          "./node_modules/ng-switchery/dist/ng-switchery.js",
          "./node_modules/deepsense-loading-spinner/dist/deepsense-loading-spinner.js",
          "./node_modules/deepsense-catalogue-panel/dist/catalogue-panel.js",
          "./node_modules/deepsense-node-parameters/dist/deepsense-node-parameters.js",
          "./node_modules/deepsense-attributes-panel/dist/deepsense-attributes-panel.js",
          "./node_modules/deepsense-navigation-panel/dist/deepsense-navigation-panel.js",
          "./node_modules/deepsense-graph-model/dist/deepsense-graph-model.js",
          "./node_modules/deepsense-cycle-analyser/dist/deepsense-cycle-analyser.js",
          "./node_modules/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.js",
          "./node_modules/angular-toastr/dist/angular-toastr.tpls.min.js",
          "./node_modules/ng-file-upload/dist/ng-file-upload.js",
          "./node_modules/ng-file-upload/dist/ng-file-upload-shim.js"
        ],
        "css": [
          "./node_modules/bootstrap/dist/css/bootstrap.css",
          "./node_modules/angular-toastr/dist/angular-toastr.min.css",
          "./node_modules/animate.css/animate.min.css",
          "./node_modules/font-awesome/css/font-awesome.css",
          "./client/css/*.css",
          "./node_modules/deepsense-loading-spinner/dist/deepsense-loading-spinner.css",
          "./node_modules/deepsense-catalogue-panel/dist/catalogue-panel.css",
          "./node_modules/deepsense-context-menu/dist/context-menu.css",
