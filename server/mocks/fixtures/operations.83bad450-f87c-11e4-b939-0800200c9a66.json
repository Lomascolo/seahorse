{
  "operation": {
    "name": "File To DataFrame",
    "description": "Converts a file to a DataFrame",
    "ports": {
      "input": [{
        "portIndex": 0,
        "typeQualifier": ["io.deepsense.deeplang.doperables.file.File"],
        "required": true
      }],
      "output": [{
        "portIndex": 0,
        "typeQualifier": ["io.deepsense.deeplang.doperables.dataframe.DataFrame"]
      }]
    },
    "version": "0.1.0",
    "id": "83bad450-f87c-11e4-b939-0800200c9a66",
    "category": "5a39e324-15f4-464c-83a5-2d7fba2858aa",
    "deterministic": false,
    "parameters": {
      "format": {
        "description": "Format of the input file",
        "default": "CSV",
        "values": {
          "CSV": {
            "separator": {
              "description": "Column separator",
              "validator": {
                "type": "regex",
                "configuration": {
                  "regex": ".*"
                }
              },
              "default": ",",
              "type": "string",
              "required": true
            },
            "namesIncluded": {
              "type": "boolean",
              "description": "Does the first row include column names?",
              "required": true,
              "default": true
            }
          }
        },
        "type": "choice",
        "required": true
      },
      "categoricalColumns": {
        "type": "selector",
        "description": "Categorical columns in the input File",
        "required": false,
        "isSingle": false
      }
    }
  }
}