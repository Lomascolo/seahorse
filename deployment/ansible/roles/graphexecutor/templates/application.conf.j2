entityStorage {
  actorSystemName = "{{entitystorage_actorsystem}}"
  hostname = "{{entitystorage_host}}"
  port = {{entitystorage_port}}
  actorName = "{{entitystorage_actor}}"
  timeoutSeconds = {{entitystorage_timeout_seconds}}

  client {
    localAddress = "{{ge_local_address}}"
    localPort = {{ge_local_port}}
  }
}

spark {
  executor.memory = "{{spark_executor_memory}}"
  driver.memory = "{{spark_driver_memory}}"
}

hdfs {
  hostname = "{{hdfs_master_hostname}}"
  port = {{hdfs_port}}
}

graphexecutor-yarn-worker {
  akka {
    actor {
      provider = "akka.remote.RemoteActorRefProvider"
    }
    remote {
      enabled-transports = ["akka.remote.netty.tcp"]
      netty.tcp {
        hostname = "value-will-be-overwritten-on-runtime"
        port = 0
      }
      log-remote-lifecycle-events = on
    }
    log-sent-messages = on
    log-received-messages = on
  }
}

master.hostname = "{{hdfs_master_hostname}}"
hdfs.name.node.port = {{hdfs_port}}

graphexecutor {
  yarn.heartbeat.interval = 100
}
