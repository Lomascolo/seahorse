{
 "display_name": "pySpark",
 "language": "python",
 "argv": [
  "/opt/conda/envs/python2/bin/python",
  "-m",
  "pyspark_kernel",
  "-f",
  "{connection_file}"
 ],
 "env": {
  "SPARK_HOME": "/opt/spark",
  "PYTHONPATH": "/opt/spark/python/:/opt/spark/python/lib/py4j-0.8.2.1-src.zip:/usr/local/share/jupyter/kernels/pyspark/",
  "KERNEL_INIT_FILE": "/usr/local/share/jupyter/kernels/pyspark/kernel_init.py",
  "PYSPARK_SUBMIT_ARGS": "--master local[*] pyspark-shell",
  "NOTEBOOK_SERVER_ADDRESS": "localhost",
  "NOTEBOOK_SERVER_PORT": "8888",
  "RABBIT_MQ_ADDRESS": "localhost",
  "RABBIT_MQ_PORT": "5672"
 }
}
