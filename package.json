{
  "name": "DeepSense.io",
  "version": "0.1.0",
  "private": true,
  "description": "DeepSense.io LAB application",
  "author": "CodiLime, Inc.",
  "license": "ISC",
  "homepage": "http://deepsense.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/codilime/deepsense-frontend.git"
  },
  "main": "./gulpfile.js",
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "gulp start",
    "predev": "npm install",
    "dev": "gulp start --dev",
    "server": "node server/app.js"
  },
  "dependencies": {
    "body-parser": "^1.10.1",
    "express": "^4.11.0",
    "http-proxy": "latest",
    "q": "latest",
    "sails-disk": "^0.10.7",
    "sails-mysql": "^0.10.7",
    "waterline": "^0.10.16"
  },
  "devDependencies": {
    "6to5ify": "^3.1.2",
    "angular": "^1.3.11",
    "angular-bootstrap": "^0.12.0",
    "angular-ui-router": "^0.2.13",
    "bootstrap": "^3.3.2",
    "bower": "^1.3.12",
    "browser-sync": "^1.9.1",
    "browserify": "^8.1.1",
    "browserify-ngannotate": "^0.6.0",
    "browserify-shim": "^3.8.2",
    "del": "^1.1.1",
    "gulp": "^3.8.10",
    "gulp-concat": "^2.4.3",
    "gulp-jshint": "^1.9.0",
    "gulp-less": "^2.0.1",
    "gulp-minify-css": "^0.4.3",
    "gulp-size": "^1.2.0",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.2",
    "jasmine": "^2.1.1",
    "jshint-stylish": "^1.0.0",
    "karma": "^0.12.31",
    "protractor": "^1.6.1",
    "run-sequence": "^1.0.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "http-server": "latest"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "angular": "global:angular",
    "jsPlumb": "global:jsPlumb"
  },
  "files": {
    "build": {
      "path": "./build/",
      "js": "js/",
      "css": "css/",
      "bundle": {
        "app": "app.js",
        "js": "libs.js",
        "css": "libs.css"
      }
    },
    "client": {
      "path": "./client/",
      "app": "app/app.js",
      "js": "app/**/*.js",
      "less": "css/**/*.less",
      "html": "**/*.html"
    },
    "libs": {
      "js": [
        "./node_modules/angular/angular.min.js",
        "./node_modules/angular-ui-router/release/angular-ui-router.min.js",
        "./node_modules/angular-bootstrap/dist/ui-bootstrap.min.js",
        "./bower_components/jsPlumb/dist/js/dom.jsPlumb-1.7.2-min.js"
      ],
      "css": [
        "./node_modules/bootstrap/dist/css/bootstrap.min.css"
      ]
    },
    "tests": {
      "e2e": "./e2e-tests/**/*.spec.js"
    }
  },
  "dev": {
    "server": {
      "port": 3000
    }
  }
}
